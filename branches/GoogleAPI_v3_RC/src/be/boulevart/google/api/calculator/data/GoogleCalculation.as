package be.boulevart.google.api.calculator.data {	/**	 * @author Joris Timmerman	 * This is the result object for GoogleCalculator	 */	public class GoogleCalculation {		private var _input:String = "";		private var _output:String = "";		private var _currencyConversion:Boolean = false;		/**		 * Your input		 */		public function get input():String {			return _input;		}		public function set input(input:String):void {			_input = input;		}		/**		 * Result for your input		 */		public function get result():String {			return _output;		}		public function set result(result:String):void {			_output = result;		}		/**		 * If this was a currency conversion, result is true		 * @default false		 */		public function get isCurrencyConversion():Boolean {			return _currencyConversion;		}		public function set isCurrencyConversion(currencyConversion:Boolean):void {			_currencyConversion = currencyConversion;		}	}}